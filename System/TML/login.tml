<Label text = "--Login--" X = "$52 / 2 - #self.text / 2" Y = 2 width = "$#self.text" height = 1 />
<Label text = "Enter a username and password" width = "$#self.text" height = 1 X = "$52 / 2 - #self.text / 2" Y = 4 />
<Label text = "Username: " width = "$#self.text" height = 1 X = "$52 / 2 - 25 / 2" Y = 6 id = "userPass"/>
<Input placeholder = "Username" width = 15 height = 1 X = "$52 / 2 - 5 / 2" Y = 6 backgroundColor = 128 color = 1 id = "user" />
<Label text = "Press enter to check username and continue" width = "$#self.text" height = 1 X = 1 Y = 19 id = "status" />
